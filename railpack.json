{
  "$schema": "https://schema.railpack.com",
  "steps": {
    "build": {
      "commands": [
        "npm install",
        "npm run build",
        "npx playwright install-deps chromium",
        "npx playwright install chromium"
      ]
    }
  },
  "deploy": {
    "startCommand": "node dist/server.js"
  }
}
